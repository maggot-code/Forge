{
    "name": "forge",
    "version": "1.0.1-0",
    "type": "module",
    "description": "",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        },
        "./*": "./*"
    },
    "files": [
        "dist",
        "*.d.ts"
    ],
    "scripts": {
        "build": "tsup && esno scripts/build.fix.ts",
        "dev": "tsup --watch",
        "start": "esno packages/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/maggot-code/Forge.git"
    },
    "author": "maggotCode",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/maggot-code/Forge/issues"
    },
    "homepage": "https://github.com/maggot-code/Forge#readme",
    "devDependencies": {
        "@types/node": "^18.11.17",
        "esno": "^0.16.3",
        "fast-glob": "^3.2.12",
        "rollup": "^3.8.1",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4"
    }
}
